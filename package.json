  "scripts": {
    "webpack": "webpack",
    "webpack-production": "webpack --config webpack-production.config.js",
    "dev": "webpack-dev-server --content-base ./ --inline --hot --history-api-fallback --host 192.168.1.245 --quiet",
    "dev-server": "SET NODE_ENV=production && babel-node src/server.js",
    "clean-server": "rm -r ./build",
    "build-server": "npm run clean-server && babel src -d build",
    "start-server": "forever start -a -p ./ -l forever.log -o out.log -e err.log ./build/server.js",
    "stop-server": "forever stop ./build/server.js",
    "update-server": "git pull origin master"
